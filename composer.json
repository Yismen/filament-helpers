{
    "name": "dainsys/filament-helpers",
    "description": "Helpers for Filament, test files generators",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Yismen Jorge",
            "email": "yismen.jorge@gmail.com"
        }
    ],
    "require": {
        "filament/filament": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.7|^8.0|^9.0|^10.0",
        "pestphp/pest": "^1.23|^2.1|^3.1",
        "phpunit/php-code-coverage": "^9.0|^10.0|^11.0",
        "phpunit/phpunit": "^9.5.24|^10.5|^11.5",
        "spatie/pest-plugin-test-time": "^1.1|^2.2"
    },
    "autoload": {
        "psr-4": {
            "Dainsys\\FilamentHelpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dainsys\\FilamentHelpers\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dainsys\\FilamentHelpers\\FilamentHelpersServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
